<h1>Burning Airlines</h1>
<h3>Come fly with me...</h3>
<h2>Login</h2>

<div class="login">
<%= form_tag login_path do %>
<p>
  <%= label_tag :email %>
  <%= email_field_tag :email %>
</p>

<p>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</p>
</div>

<%= submit_tag "Login"%>

<% end %>
